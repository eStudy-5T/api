  "scripts": {
    "dev": "[ -f .env ]; nodemon --exec sucrase-node ./src/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:renew": "npx sequelize db:drop && npx sequelize db:create && yarn db:migrate && yarn db:seed",
    "lint": "eslint --ext .js src/",
    "fix-lint": "eslint --fix --ext .js src/"
  },
  "dependencies": {
