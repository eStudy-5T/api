{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": 1,
		"object-curly-spacing": 1,
		"no-unused-vars": 1
	}
}
